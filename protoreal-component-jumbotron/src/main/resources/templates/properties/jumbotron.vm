<div class="form" id="frmInput">
	<form id="frmInput" action="" method="">
	<input type="hidden" id="setUuid" value="$JumbotronRenderer.uuid" />
		
		<div class="form-group">
			<label for="inputProjectTitle" class="col-form-label">Titulo</label> 
			<input type="text" id="setTitle" class="form-control" value="$JumbotronRenderer.title" size="25" />
		</div>
		
		<div class="form-group">
			<label for="inputProjectTitle" class="col-form-label">Descricao</label> 
			<textarea id="setSubtitle" class="form-control" property="title" rows="10">$JumbotronRenderer.subtitle</textarea>
		</div>
		
		<div class="form-group">
			<label for="inputProjectTitle" class="col-form-label">Cor da Fonte</label> 
			<input type="color" id="setFontColor" class="form-control" value="$JumbotronRenderer.fontColor" size="25" />
		</div>
		
		<div class="form-group">
			<label for="inputProjectTitle" class="col-form-label">Text do Botão</label> 
			<input type="text" id="setButtonText" class="form-control" value="$JumbotronRenderer.buttonText" size="25" />
		</div>

		<div class="form-group">
			<label for="inputProjectTitle" class="col-form-label">Text do Botão</label> 
			<input type="text" id="setButtonUrl" class="form-control" value="$JumbotronRenderer.buttonUrl" size="25" />
		</div>
		<p>
		<div class="modal-footer">
			<button type="button" class="btn btn-secondary"	data-dismiss="modal">Fechar</button>
			<button type="button" class="btn btn-primary" onclick="edit('frmInput');">Salvar</button>
		</div>
	</div>
	</form>
</div>