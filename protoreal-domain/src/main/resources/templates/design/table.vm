<table class="table table-sm table-striped table-hover">
  <thead class="thead-light">
    <tr>
    #foreach($item in $TableRenderer.getHeader())
      <th scope="col">$item</th>
    #end
    </tr>
  </thead>

  <tbody>
    #foreach($row in $TableRenderer.getRows())
    <tr>
      #foreach($itm in $row)
      <td>$itm</td>
      #end
    </tr>
  	#end
  </tbody>
</table>

<hr class="my-4">

